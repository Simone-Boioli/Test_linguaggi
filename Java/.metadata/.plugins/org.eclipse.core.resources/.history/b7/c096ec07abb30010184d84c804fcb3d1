package RubricaGUI;

import java.awt.*;

public class FinestraPrincipale extends Frame {
	Button cerca;
	Button insert;
	Button delete;
	
	public FinestraPrincipale(){
		setSize(400,200);
		setLocation(400,400);
		setVisible(true);
		
		cerca = new Button("Cerca Contatto");
		insert = new Button("Inserisci Contatto");
		delete = new Button("Elimina Contatto");
		
		add(cerca, BorderLayout.WEST);
		add(insert, BorderLayout.CENTER);
		add(delete, BorderLayout.EAST);
		
	}
	
}
