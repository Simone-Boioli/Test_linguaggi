package RubricaGUI;

import java.awt.*;

public class FinestraCerca extends Frame{
	Label titolo;
	public FinestraCerca(){
		pack();
		setVisible(true);
		setSize(300,300);
		setLocation(50,50);
		
		titolo = new Label("                        CERCA UTENTE");
		
		add(titolo, BorderLayout.NORTH);
	}
}
