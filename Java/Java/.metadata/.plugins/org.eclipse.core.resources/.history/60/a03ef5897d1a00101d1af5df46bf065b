
public class Dvd {
	private int regionCode; //codice dvd
	private String titolo;
	private int durata;
	
	public Dvd(){
		
	}
	
	public Dvd(int cod, String titolo, int durata){
		regionCode = cod;
		this.titolo = titolo;
		this.durata = durata;
	}

	public String toString() {
		return "Dvd [Codice=" + regionCode + ", titolo=" + titolo
				+ ", durata=" + durata + "]";
	}

	public int getRegionCode() {
		return regionCode;
	}
	
	
	
	
}
