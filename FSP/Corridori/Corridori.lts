				partenza -> salta11 -> salta12 -> arrivo1 -> STOP
			).

CORRIDORE2 = (
				partenza -> salta21 -> salta22 -> arrivo2 -> STOP
			).

||CORRIDORI = (CORRIDORE1 || CORRIDORE2).